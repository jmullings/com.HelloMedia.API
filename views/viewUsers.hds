<link rel="stylesheet" href="/stylesheets/style.css">
<h1>MONGODB - User file</h1>

<section class="get">
  <h3>Get Data</h3>
  <a href="/viewUsers">LOAD DATA</a>
  <div>
    {{# each items }}
    <article class="item">
      <div>Title: {{ this.title }}</div>
      <div>Content: {{ this.content }}</div>
      <div>Author: {{ this.author }}</div>
      <div>ID: {{ this._id }}</div>
    </article>
    {{/each}}
  </div>
</section>
